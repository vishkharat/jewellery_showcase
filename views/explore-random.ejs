<h1 class="pb-4">Explore Random</h1>


<div class="row row-cols-2 row-cols-lg-5 g-2 g-lg-3">

    <% if(designs != '') { %>
      <a href="/design/<%= designs._id %>" class="col text-center category__link">
        <div class="category__img category__img--large shadow">
          <img src="/uploads/<%= designs .image %>" alt="<%= designs.name %>" loading="lazy">
        </div>
        <div class="pt-1"><%= designs.name %></div>
      </a>
    <% } else { %>
      <p>No items found.</p>
    <% } %>

     
  </div>