<h1 class="pb-4">Explore Latest</h1>


<div class="row row-cols-2 row-cols-lg-5 g-2 g-lg-3">

    <% if(designs != '') { %>
      <% designs.forEach(function(design, index){ %>
      <a href="/design/<%= design._id %>" class="col text-center category__link">
        <div class="category__img category__img--large shadow">
          <img src="/uploads/<%= design .image %>" alt="<%= design.name %>" loading="lazy">
        </div>
        <div class="pt-1"><%= design.name %></div>
      </a>
      <% }) %>
    <% } else { %>
      <p>No items found.</p>
    <% } %>

     
  </div>